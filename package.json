{
  "name": "passprotect-vue2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "codegen": "gql-gen --config codegen.yml"
  },
  "dependencies": {
    "@types/downloadjs": "^1.4.1",
    "@types/lodash.merge": "^4.6.6",
    "clipboard-copy": "^3.0.0",
    "downloadjs": "^1.4.7",
    "es6-promisify": "^6.0.1",
    "json2csv": "^4.5.1",
    "localforage": "^1.7.3",
    "lodash.merge": "^4.6.1",
    "register-service-worker": "^1.6.2",
    "stream-to-promise": "^2.2.0",
    "vee-validate": "^2.2.11",
    "vue": "^2.6.10",
    "vue-apollo": "^3.0.0-rc.1",
    "vue-apollo-decorator": "^0.1.2",
    "vue-class-component": "^7.1.0",
    "vue-i18n": "^8.11.2",
    "vue-property-decorator": "^8.2.1",
    "vue-router": "^3.0.6",
    "vuetify": "^1.5.16",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.3.1",
    "@graphql-codegen/fragment-matcher": "^1.3.1",
    "@graphql-codegen/typescript": "^1.3.1",
    "@graphql-codegen/typescript-operations": "^1.3.1",
    "@types/es6-promisify": "^6.0.0",
    "@types/graphql": "^14.2.1",
    "@types/jest": "^24.0.15",
    "@types/json2csv": "^4.5.0",
    "@types/localforage": "0.0.34",
    "@types/stream-to-promise": "^2.2.0",
    "@types/webpack": "^4.4.33",
    "@types/zxcvbn": "^4.4.0",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-e2e-cypress": "^3.8.0",
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.1",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.4",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "graphql-tag": "^2.9.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^24.0.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2",
    "vue-cli-plugin-apollo": "^0.20.0",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-graphql-inject": "^1.0.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2",
    "yaml-loader": "^0.5.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "globals": {
      "ts-jest": {
        "babelConfig": true
      }
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
